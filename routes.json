{
    "rest": {
        "get": {
            "/api/playlist/([0-9]+)$": "/rest/playlist?playlist=$1",
            "/api/collection$": "/rest/collection",
            "/api/user/([0-9]+)$": "/rest/user?user=$1"
        },
        "put": {
            "/api/playlist/([0-9]+)$": "/rest/playlist?playlist=$1"
        },
        "delete": {
            "/api/playlist/([0-9]+)$": "/rest/playlist?track=$1"
        }
    },
    "web": {
        "get": {
            "/$": "/views/index.html",
            "/search\\?([\\S+]*)?$": "/views/search.html",
            "/index$": "/views/index.html",
            "/home$": "/views/home.html",
            "/album$": "/views/album.html",
            "/cart$": "/views/cart.html"
        },
        "post": {
            "^\/search\\?([\\S+]*)?$": "/views/search.html"
        }
    }
}